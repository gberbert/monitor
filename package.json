{
    "name": "monitor-ag-root",
    "version": "1.0.27",
    "description": "Root orchestrator for Monitor AG",
    "scripts": {
        "dev": "npx -y kill-port 3001 5173 && concurrently -k \"npm run server\" \"npm run client\"",
        "server": "node server/index.js",
        "client": "cd client && npm run dev",
        "kill": "npx -y kill-port 3001 5173"
    },
    "devDependencies": {
        "concurrently": "^8.0.0"
    }
}